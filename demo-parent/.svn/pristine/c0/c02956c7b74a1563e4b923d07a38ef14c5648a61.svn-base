/******************************************************
  RBrowserDialog
******************************************************/
.rb-BrowserDialog-filterpanel {
  border: 1px solid #B7D2FA;
}

/******************************************************
  RBrowseBox
******************************************************/
.rb-BrowseBox-nonEditable {
  border-color: #b0b0b0;
  background: #e7edf4;  
}

.rb-BrowseBox .rbi-BrowseBox-browseButton {
  background: url(../images/form/browse/browseBtn.gif) no-repeat 3px center;
}

.rb-BrowseBox .rbi-BrowseBox-browseButton-disabled {
  background: url(../images/form/browse/browseBtn-dis.gif) no-repeat 3px center;
  cursor: default;
}

.rb-BrowseBox .rbi-BrowseBox-browseButton-hover {
  background: #fffbf0 url(../images/form/browse/browseBtn-hov.gif) no-repeat center center;
  border-left: 1px solid #c8ba94;
}

.rb-BrowseBox .rbi-BrowseBox-browseButton-mouseDown {
  background: #ffeaae url(../images/form/browse/browseBtn-mouseDown.gif) no-repeat center center;
  border-left: 1px solid #b0a380;
}

.rb-BrowseBox .rbi-BrowseBox-browseButton-fieldHover {
  background: #e4f4fd url(../images/form/browse/browseBtn-fldHov.gif) no-repeat center center;
  border-left: 1px solid #b2c6df;
}

.rb-BrowseBox .rbi-BrowseBox-browseButton-fieldFocus {
  background: #fffbf0 url(../images/form/browse/browseBtn-fldFocus.gif) no-repeat center center;
  border-left: 1px solid #b0a380;
}

/******************************************************
  RUCNBox
******************************************************/
.rb-OldUCNBox {
  float: left;
}

.rb-OldUCNBox-browsebutton {
  float: left;
  padding: 2px 0px 0px 0px;
  cursor: hand;
}

