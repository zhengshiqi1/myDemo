/******************************************************
  RSearchBox
******************************************************/
.rb-SearchBox { border: 1px solid #b5b8c8;
    background: white url(../images/form/text-bg.gif) repeat-x 0 0; }
.rb-SearchBox .inputCell { vertical-align: middle; }
.rb-SearchBox .input { color: #333333; height: 19px; width: 150px; border: 0; }
