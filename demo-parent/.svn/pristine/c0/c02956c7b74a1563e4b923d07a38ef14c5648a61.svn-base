/******************************************************
  ROperateInfoPanel
******************************************************/
.rb-OperateInfoPanel { margin: 3px; }
.rb-OperateInfoPanel .operator { font-weight: bold; }
.rb-OperateInfoPanel .time { margin: 3px 0px 5px 0px; }
.rb-OperateInfoPanel .deck { border-top: 1px solid darkgray; }
.rb-OperateInfoPanel .loading { height: 50px; text-align: center; vertical-align: middle; }
.rb-OperateInfoPanel .loading .inner { height: 16px; margin: 5px;
    background: transparent url('../images/form/loading.gif') no-repeat 50% 50%; }
.rb-OperateInfoPanel .empty { height: 50px; text-align: center; vertical-align: middle; }
.rb-OperateInfoPanel .empty .inner { color: darkgray; margin: 5px; }
.rb-OperateInfoPanel .contact {}
.rb-OperateInfoPanel .contact .label { white-space: nowrap; padding-right: 3px; }
.rb-OperateInfoPanel .contact .value {}
